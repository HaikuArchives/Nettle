#
# brane-ded config
#
rm -f make.incl
HOST=`uname`
 
case $HOST in
 	*BeOS*) ln -s be.incl make.incl;;
 	*Linux*) ln -s linux.incl make.incl;;
 	*IRIX*) ln -s irix.incl make.incl;;
 	*SunOS*) if(whence CC)
 			 then
 			 	ln -s sparcworks.incl make.incl
 			 else
 			 	ln -s solaris-gcc.incl make.incl
 			 fi;;
esac
    
