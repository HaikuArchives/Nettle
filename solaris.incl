CPP=g++
CC=gcc
CCFLAGS= -g -D_REENTRANT -DSOLARIS $(INCLUDE)
CPPFLAGS= -g -fhandle-exceptions -Wall -D_REENTRANT -DSOLARIS $(INCLUDE)
LINKLIBS= -lnettle -lnsl -lsocket 
PLATOBJS= NLSolarisImpl.o 
AR=ar -q

.SUFFIXES: .cpp .o .c 

.cpp.o:
	$(CPP)  -c $(CPPFLAGS) $<

.c.o:
	$(CC)  -c $(CCFLAGS) $<
	
	
